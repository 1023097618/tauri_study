{
    "compilerOptions":{
        "target":"es6",
        "module":"commonjs",
        "outDir":"./dist",
        "allowJs":true,
        "checkJs":true,
        "removeComments":true,
        "noEmitOnError":true,
        "alwaysStrict":true,
        "noImplicitAny":true,
        "noImplicitThis":true,
        "strictNullChecks":true,
        "strict":true
    }
}